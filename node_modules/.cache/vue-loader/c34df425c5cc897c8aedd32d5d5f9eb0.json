n  <div class=\"user-wrapper\">\r\n    <a-dropdown>\r\n      <div>\r\n        <a-avatar v-if=\"userInfo.head\" class=\"avatar\" :src=\"userInfo.head\" />\r\n        <a-avatar v-else style=\"color: #f56a00; backgroundColor: #fde3cf\">{{\r\n          userInfo.username\r\n        }}</a-avatar>\r\n        <span style=\"\tmargin-left: 5px;color: #fff;\">{{\r\n          userInfo.nickname\r\n        }}</span>\r\n      </div>\r\n      <a-menu slot=\"overlay\">\r\n        <a-menu-item>\r\n          <a-button type=\"link\" icon=\"setting\" @click=\"clickUserCenter\">\r\n            {{ $t('common.Mysettings') }}</a-button\r\n          >\r\n        </a-menu-item>\r\n        <a-menu-item>\r\n          <a-button type=\"link\" style=\"color:#888\" icon=\"export\" @click=\"handleLogout\">{{\r\n            $t('user.Logout')\r\n          }}</a-button>\r\n        </a-menu-item>\r\n      </a-menu>\r\n    </a-dropdown>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapState, mapActions } from 'vuex'\r\nexport default {\r\n  computed: {\r\n    ...mapState({\r\n      userInfo: state => state.user.info\r\n    })\r\n  },\r\n  //方法集合\r\n  methods: {\r\n    ...mapActions(['SetToken']),\r\n    clickUserCenter() {\r\n      this.$router.push({ name: 'apps', params: { id: this.userInfo.id } })\r\n    },\r\n    //退出登录\r\n    handleLogout() {\r\n      // 您确定要退出登录吗？\r\n      const _this = this\r\n      this.$confirm({\r\n        title: this.$t('common.youlayout'),\r\n        onOk() {\r\n          _this.SetToken(null)\r\n          _this.$router.push({ name: 'index' })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.content-box {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"]}]}