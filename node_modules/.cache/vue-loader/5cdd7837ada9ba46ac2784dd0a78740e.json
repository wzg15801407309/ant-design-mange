ource=[];\r\n      this.rankList=res.body.map(item=>{\r\n        source.push(item.source)\r\n        return {\r\n          name:moment(item.data).format('YYYY-MM-DD'),\r\n          total:item.source\r\n        }\r\n      });\r\n      let temp=Object.entries(this.getWordCnt(source))\r\n      this.barData=temp.map(item=>{\r\n        return {\r\n          x:item[0],\r\n          y:item[1]\r\n        }\r\n      })\r\n      // let sourceList=await getSourceAlList(id)\r\n      // console.log(sourceList,12345)\r\n      // this.barData= sourceList.body.map(item=>{\r\n      //  return{\r\n      //     y:item.total,\r\n      //     x:item._id\r\n      //   }\r\n      // })\r\n      this.sourceData=temp.map(values=>{\r\n        return { item: values[0], count: values[1] }\r\n      })\r\n      const dv = new DataSet.View().source(this.sourceData)\r\n      dv.transform({\r\n        type: 'percent',\r\n        field: 'count',\r\n        dimension: 'item',\r\n        as: 'percent'\r\n      })\r\n      this.pieData = dv.rows\r\n    },\r\n  },\r\n  created(){\r\n    this.getProjectAllRecordInfo();\r\n  }\r\n}\r\n</script>"]}]}